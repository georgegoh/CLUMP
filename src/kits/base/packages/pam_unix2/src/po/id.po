#: src/unix_acct.c:102 src/unix_acct.c:150
msgid "Your password has expired. Choose a new password."
msgstr "Password anda telah kadaluarsa. Pilih sebuah password baru."

#: src/unix_acct.c:129
msgid "Password change requested. Choose a new password."
msgstr "Permintaan perubahan password. Pilih sebuah password baru."

#: src/unix_acct.c:288
#, c-format
msgid "Your password will expire in %ld days."
msgstr "Password anda akan kadaluarsa dalam %ld hari."

#: src/unix_acct.c:292
msgid "Your password will expire tomorrow."
msgstr "Password anda akan kadaluarsa besok."

#: src/unix_acct.c:295
msgid "Your password will expire within 24 hours."
msgstr "Password anda akan kadaluarsa dalam 24 jam."

