#: src/support.c:153
msgid "login: "
msgstr "влизане: "

#: src/support.c:167 src/support.c:212
msgid "Password: "
msgstr "Парола: "

#: src/unix_acct.c:102 src/unix_acct.c:150
msgid "Your password has expired. Choose a new password."
msgstr "Паролата Ви е изтекла. Изберете нова парола."

#: src/unix_acct.c:129
msgid "Password change requested. Choose a new password."
msgstr "Има заявка за промяна на паролата. Изберете нова парола."

#: src/unix_acct.c:288
#, c-format
msgid "Your password will expire in %ld days."
msgstr "Паролата Ви ще изтече след %ld дни."

#: src/unix_acct.c:292
msgid "Your password will expire tomorrow."
msgstr "Паролата Ви ще изтече утре."

#: src/unix_acct.c:295
msgid "Your password will expire within 24 hours."
msgstr "Паролата Ви ще изтече след 24 часа."

#: src/unix_auth.c:312
msgid "Permissions on the password database may be too restrictive."
msgstr "Правата за базата с пароли може би са твърде ограничителни."

#: src/unix_passwd.c:83
msgid "Old Password: "
msgstr "Стара парола: "

#: src/unix_passwd.c:84
msgid "New Password: "
msgstr "Нова парола: "

#: src/unix_passwd.c:85
msgid "Reenter New Password: "
msgstr "Въведете повторно парола: "

#: src/unix_passwd.c:214
msgid "You can only change local passwords."
msgstr "Можете да променяте само локални пароли."

#: src/unix_passwd.c:284
#, c-format
msgid "Less then %d weeks since the last change."
msgstr "По-малко от %d седмици от последната промяна."

#: src/unix_passwd.c:288
#, c-format
msgid "Less then %d days since the last change."
msgstr "По-малко от %d дни от последната промяна."

#: src/unix_passwd.c:340 src/unix_passwd.c:355
msgid "Password change aborted."
msgstr "Промяната на паролата е прекъсната."

#: src/unix_passwd.c:369
msgid "Passwords do not match."
msgstr "Паролите не съответстват."

#: src/unix_passwd.c:394
msgid "Error: Password NOT changed."
msgstr "Грешка: Паролата не е променена."

#: src/unix_passwd.c:402
msgid "Password changed."
msgstr "Паролата е променена."

#: src/unix_passwd.c:419
#, c-format
msgid "Cannot find the master ypserver: %s"
msgstr "Не може да бъде открит главния ypserver: %s"

#: src/unix_passwd.c:427
#, c-format
msgid "yppasswdd not running on NIS master %s"
msgstr "yppasswdd не работи на главния NIS %s"

#: src/unix_passwd.c:433
msgid "yppasswd daemon running on illegal port."
msgstr "Демонът yppasswd работи на неправилен порт."

#: src/unix_passwd.c:484
#, c-format
msgid "Cannot open %s for reading: %s"
msgstr "Файлът %s не може да бъде отворен за четене: %s"

#: src/unix_passwd.c:493
#, c-format
msgid "Unable to obtain entropy from %s"
msgstr "Не може да се придобие ентропията от %s"

#: src/unix_passwd.c:508
msgid "Unable to generate a salt. Check your crypt settings."
msgstr "Не може да се генерира солта. Проверете настройките криптирането."

#: src/unix_passwd.c:634
msgid "Cannot create salt for standard crypt"
msgstr "Не може да се създаде сол за стандартния crypt"

#: src/unix_passwd.c:646
msgid "Cannot create salt for MD5 crypt"
msgstr "Не може да се създаде сол за стандартния MD5"

#: src/unix_passwd.c:659
msgid "Cannot create salt for bigcrypt"
msgstr "Не може да се създаде сол за bigcrypt"

#: src/unix_passwd.c:665
msgid "No support for bigcrypt included"
msgstr "Няма включена поддръжка за bigcrypt"

#: src/unix_passwd.c:677
msgid "Cannot create salt for blowfish crypt"
msgstr "Не може да се създаде сол за blowfish криптиране"

#: src/unix_passwd.c:683
msgid "No support for blowfish included"
msgstr "Няма включена поддръжка за blowfish"

#: src/unix_passwd.c:695
msgid "crypt_r() returns NULL pointer"
msgstr "crypt_r() върна NULL указател"

#: src/unix_passwd.c:718
msgid "Cannot lock password file: already locked."
msgstr "Файлът с паролите не може да бъде заключен: вече е заключен."

#: src/unix_passwd.c:745 src/unix_passwd.c:806 src/unix_passwd.c:889
#: src/unix_passwd.c:950
#, c-format
msgid "Cannot open %s: %m"
msgstr "Възникнала е грешка при отварянето на %s: %m"

#: src/unix_passwd.c:752 src/unix_passwd.c:896
#, c-format
msgid "Cannot stat %s: %m"
msgstr "Възникнала е грешка при изваждането на състоянието %s: %m"

#: src/unix_passwd.c:780 src/unix_passwd.c:924
#, c-format
msgid "Cannot create temp file (%s): %m"
msgstr "Временният файл (%s) не може да бъде създаден: %m"

#: src/unix_passwd.c:788 src/unix_passwd.c:932
#, c-format
msgid "Cannot change permssions of %s: %m"
msgstr "Правата на %s не могат да бъдат променени: %m"

#: src/unix_passwd.c:793 src/unix_passwd.c:936
#, c-format
msgid "Cannot change owner/group of %s: %m"
msgstr "Собственика/групата на %s не може да бъде променен: %m"

#: src/unix_passwd.c:836
#, c-format
msgid "Error while writing new shadow file: %m"
msgstr "Възникнала е грешка при записването на нов shadow файл: %m"

#: src/unix_passwd.c:846
#, c-format
msgid "Error while closing old shadow file: %m"
msgstr "Възникнала е грешка при затварянето на стария shadow файл: %m"

#: src/unix_passwd.c:854
#, c-format
msgid "Error while closing temporary shadow file: %m"
msgstr "Възникнала е грешка при затварянето на временния shadow файл: %m"

#: src/unix_passwd.c:861 src/unix_passwd.c:1001
#, c-format
msgid "Cannot create backup file of %s: %m"
msgstr "Създаването на резервно копие на %s бе неуспешно: %m"

#: src/unix_passwd.c:976
#, c-format
msgid "Error while writing new password file: %m"
msgstr "Възникнала е грешка при записването на новия файл с пароли: %m"

#: src/unix_passwd.c:986
#, c-format
msgid "Error while closing old password file: %m"
msgstr "Възникнала е грешка при затварянето на стария файл с пароли: %m"

#: src/unix_passwd.c:994
#, c-format
msgid "Error while closing temporary password file: %m"
msgstr "Възникнала е грешка при затварянето на временния файл с пароли: %m"

#: src/unix_passwd.c:1033
#, c-format
msgid "Changing NIS password for %s on %s."
msgstr "Промяна на паролата в NIS за %s на %s."

#: src/unix_passwd.c:1048
msgid "Error while changing the NIS password."
msgstr "Възникнала е грешка при промяната на паролата в NIS."
